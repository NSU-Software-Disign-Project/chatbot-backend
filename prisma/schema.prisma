generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Project {
  id            String       @id @default(auto()) @map("_id") @db.ObjectId
  name          String       @unique
  createdAt     DateTime     @default(now())
  updatedAt     DateTime     @updatedAt
  nodeDataArray Json[]
  linkDataArray Json[]
}
